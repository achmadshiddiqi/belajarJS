<!-- Header -->
<%- include('./layouts/header') %>
<!-- Content -->
<div class="row">
  <div class="col-md-4">
    <h1 class="text-center">Users</h1>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">No.</th>
          <th scope="col">Username</th>
          <th scope="col">Role</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach((user, i) => { %>
        <tr>
          <th scope="row"><%= i+1 %></th>
          <td><%= user.username %></td>
          <td><%= user.role %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</div>
<!-- Footer -->
<%- include('./layouts/footer') %>
